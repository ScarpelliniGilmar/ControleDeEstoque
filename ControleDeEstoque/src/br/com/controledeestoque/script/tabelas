create database produtos
go

use produtos
go

create table usuarios(
	email char(32) primary key,
	senha char(32)
	)
	go

create table produtos (
codigo_produto int IDENTITY (1,1) primary key,
descricao_produto varchar(50),
valor_produto money,
quantidade int
)
go

create table entradas (
codigo_entradas int IDENTITY (1,1) primary key,
codigo_produto int,
quantidade int,
data date
)
go

create table vendas (
codigo_venda int,
descricao_produto varchar(50),
quantidade int,
valor money,
data date
)
go